ound-position: 50% 0;\n  text-align: center;\n}\n\n.container div {\n  padding: 20px;\n}\n.top {\n  height: 75vh;\n  background: $blacktransparant;\n}\n\n.top h1 {\n  @include bold-font();\n  color: $light;\n  text-transform: uppercase;\n  padding-top: 50px;\n  opacity: 0.8;\n  font-size: 70px;\n}\n\n.top p {\n  @include normal-font();\n  color: $accent;\n  text-transform: uppercase;\n  letter-spacing: 1em;\n  margin-top: -25px;\n  margin-bottom: 20px;\n  line-height: 1.5em;\n}\n\n.social-icons {\n  margin-top: 20px;\n  color: $accent;\n  text-align: center;\n  list-style-type: none;\n  display: inline;\n  text-align: center;\n}\n\n.social-icons--item {\n  margin: 5px 10px 5px 10px;\n}\n\n.social-icons--item:hover {\n  -webkit-animation: rotate-center 0.6s ease-in-out both;\n  animation: rotate-center 0.6s ease-in-out both;\n}\n\n.middle {\n  background: $accent;\n  clip-path: polygon(0% 0%, 100% 0, 100% 75%, 50% 100%, 0 75%);\n  position: absolute;\n  top: 80vh;\n  width: 100%;\n  height: 10vh;\n  border-top: 5px solid $lighttransparant;\n}\n\n.middle h2 {\n  color: $light;\n  @include bold-font();\n  text-transform: uppercase;\n  margin-bottom: 15px;\n  font-size: 19px;\n}\n\n.middle a {\n  color: $accent;\n  font-size: 19px;\n  @include bold-font();\n  text-transform: uppercase;\n  padding: 5px 20px;\n  text-decoration: none;\n  background: $darkGreen;\n  border-radius: 5%;\n}\n\n.middle a:hover {\n  cursor: pointer;\n  opacity: 0.8;\n}\n\n.section {\n  color: $light;\n  background: $darkGreen;\n}\n\n.section .first-item {\n  margin-top: 110px;\n  @include normal-font();\n  text-align: left;\n  line-height: 1.5em;\n  width: 70%;\n  opacity: 0.8;\n}\n\n.section p {\n  @include normal-font();\n  text-align: left;\n  line-height: 1.5em;\n  width: 70%;\n  opacity: 0.8;\n  margin-bottom: 1.5em;\n}\n\n.merch-section {\n  margin-top: -28px;\n}\n\n.divider {\n  background: $accent;\n  clip-path: polygon(0% 0%, 100% 0, 100% 75%, 50% 100%, 0 75%);\n  width: 100%;\n  height